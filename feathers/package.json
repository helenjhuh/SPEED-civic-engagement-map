{
  "name": "feathers",
  "description": "The API server for the Civic Engagement project at Swarthmore College.",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Swarthmore College",
    "email": "aweed1@swarthmore.edu"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^12.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "test": "yarn run lint && yarn run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json",
    "lint:fix": "eslint src/. --config .eslintrc.json --fix",
    "dev": "nodemon src/",
    "start": "node src/",
    "mocha": "mocha test/ --recursive --exit",
    "client:build": "webpack",
    "client:watch": "webpack --watch",
    "mongo": "docker run --name civic_map_mongodb -d -p 27017:27017 -e MONGO_INITDB_ROOT_USERNAME=root -e MONGO_INITDB_ROOT_PASSWORD=root mongo",
    "mongoexpress": "docker run -d --rm -p 8080:8081 --env ME_CONFIG_MONGODB_ADMINUSERNAME='root' --env ME_CONFIG_MONGODB_ADMINPASSWORD='root' --link civic_map_mongodb:mongo mongo-express",
    "pre-push": "yarn lint:fix"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.1",
    "@feathersjs/authentication-client": "^4.5.1",
    "@feathersjs/authentication-local": "^4.5.1",
    "@feathersjs/authentication-oauth": "^4.5.1",
    "@feathersjs/configuration": "^4.5.1",
    "@feathersjs/errors": "^4.5.1",
    "@feathersjs/express": "^4.5.1",
    "@feathersjs/feathers": "^4.5.1",
    "@feathersjs/socketio": "^4.5.1",
    "@feathersjs/socketio-client": "^4.5.1",
    "@feathersjs/transport-commons": "^4.5.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@mapbox/mapbox-sdk": "^0.9.0",
    "bootstrap": "^4.4.1",
    "compression": "^1.7.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "feathers-mongoose": "^8.3.0",
    "feathers-permissions": "^2.0.1",
    "formik": "^2.1.4",
    "gridfs-stream": "^1.1.1",
    "helmet": "^3.21.2",
    "mapbox-gl": "^1.8.1",
    "meow": "^6.0.1",
    "mime-type": "^3.0.7",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.8.13",
    "multer": "^1.4.2",
    "multer-gridfs-storage": "^4.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-mapbox-gl": "^4.8.2",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^2.3.0",
    "winston": "^3.0.0",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "axios": "^0.19.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.3",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "mocha": "^7.0.1",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "react-bootstrap": "^1.0.0-beta.16",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  }
}
